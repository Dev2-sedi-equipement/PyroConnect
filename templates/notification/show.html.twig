{% extends 'base.html.twig' %}

{% block title %}Notification{% endblock %}

{% block body %}
    <h1>Notification</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ notification.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ notification.titre }}</td>
            </tr>
            <tr>
                <th>User</th>
                <td>{{ notification.user }}</td>
            </tr>
            <tr>
                <th>DateCreation</th>
                <td>{{ notification.dateCreation ? notification.dateCreation|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Id_dossier</th>
                <td>{{ notification.idDossier }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ notification.type }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_notification_index') }}">back to list</a>

    <a href="{{ path('app_notification_edit', {'id': notification.id}) }}">edit</a>

    {{ include('notification/_delete_form.html.twig') }}
{% endblock %}
